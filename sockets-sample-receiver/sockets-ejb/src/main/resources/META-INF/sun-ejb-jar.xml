			</mdb-resource-adapter>
		</ejb>
		<ejb>
			<ejb-name>SOCKET_MDB1</ejb-name>
			<mdb-resource-adapter>
                <!-- DO NOT specify the file extension .rar  -->
				<resource-adapter-mid>sockets-rar-${project.version}</resource-adapter-mid>
			</mdb-resource-adapter>
		</ejb>
	</enterprise-beans>
</sun-ejb-jar>